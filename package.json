  "license": "MIT",
  "bugs": {
    "url": "https://github.com/statscore/simple-websocket/issues"
  },
  "homepage": "https://github.com/statscore/simple-websocket#readme",
  "dependencies": {
    "dotenv": "^7.0.0",
    "semver": "^5.6.0",
    "ws": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "nodemon": "^1.18.10"
  }
}
